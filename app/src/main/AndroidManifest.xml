<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:enableOnBackInvokedCallback="true"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31">
        <activity
            android:name=".login.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.github.procrastinotteam.procrastinot.register.RegisterActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.github.procrastinotteam.procrastinot.BaseActivity"
            android:exported="false" />
        <activity
            android:name="com.github.procrastinotteam.procrastinot.settings.SettingsActivity"
            android:exported="false" />
        <activity
            android:name="com.github.procrastinotteam.procrastinot.profile.ProfileActivity"
            android:exported="false" />
        <activity
            android:name="com.github.procrastinotteam.procrastinot.search.SearchActivity"
            android:exported="false" />
        <activity
            android:name="com.github.procrastinotteam.procrastinot.habits.HabitsActivity"
            android:exported="false" />
        <activity
            android:name="com.github.procrastinotteam.procrastinot.home.HomeActivity"
            android:exported="false" />
    </application>

</manifest>